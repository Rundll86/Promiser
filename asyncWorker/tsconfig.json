{
    "compilerOptions": {
        "target": "ESNext",
        "module": "esnext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "outDir": "./dist"
    },
    "include": [
        "src/**/*.ts"
    ]
}